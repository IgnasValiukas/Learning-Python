1 - SELECT * FROM DARBUOTOJAI

2 - SELECT GIMIMO_DATA FROM DARBUOTOJAI

3 - SELECT GIMIMO_DATA, PAVARDĖ, PAREIGOS FROM DARBUOTOJAI

4 - SELECT DISTINCT SKYRIUS_PAVADINIMAS FROM DARBUOTOJAI  

5 - SELECT * FROM DARBUOTOJAI WHERE SKYRIUS_PAVADINIMAS = "Gamybos"

6 - SELECT PAREIGOS FROM DARBUOTOJAI WHERE VARDAS = "Giedrius"

7 - SELECT * FROM DARBUOTOJAI WHERE GIMIMO_DATA = date('1986-09-19')

8 - SELECT VARDAS FROM DARBUOTOJAI WHERE PAVARDĖ = "Sabutis"

9 - SELECT VARDAS, PAVARDĖ FROM DARBUOTOJAI WHERE PAREIGOS = "Programuotojas" AND SKYRIUS_PAVADINIMAS = "Gamybos"

10 - INSERT INTO DARBUOTOJAI VALUES("Tomas", "Paulys", date('2000-02-02'), "Testuotojas", "Gamybos"

11 - INSERT INTO DARBUOTOJAI (VARDAS, PAVARDĖ, GIMIMO_DATA) VALUES("Tadas", "Kaulas", date('1988-03-10'))

12 - UPDATE DARBUOTOJAI SET PAREIGOS="Programuotojas", SKYRIUS_PAVADINIMAS="Gamybos" WHERE VARDAS="Tadas" AND PAVARDĖ="Kaulas"

13 - DELETE FROM DARBUOTOJAI WHERE GIMIMO_DATA = date('1988-03-10')

14 - INSERT INTO DARBUOTOJAI VALUES("Kazys", "Antanaitis", date('2008-02-03'), "Programuotojas", "Gamybos")
     INSERT INTO DARBUOTOJAI VALUES("Jonas", "Antanaitis", date('2008-02-03'), "Programuotojas", "Gamybos")

15 - UPDATE DARBUOTOJAI SET PAREIGOS="Testuotojas" WHERE PAVARDĖ="Antanaitis"

Papildoma: SELECT count(PAREIGOS) FROM DARBUOTOJAI WHERE PAREIGOS="Testuotojas"

