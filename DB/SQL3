1.
SELECT DARBUOTOJAS.VARDAS, DARBUOTOJAS.PAVARDĖ, PROJEKTAS.PAVADINIMAS
FROM DARBUOTOJAS, PROJEKTAS
WHERE DARBUOTOJAS.PROJEKTAS_ID = PROJEKTAS.ID

2.
SELECT DARBUOTOJAS.VARDAS, DARBUOTOJAS.PAVARDĖ, PROJEKTAS.PAVADINIMAS
FROM DARBUOTOJAS, PROJEKTAS
WHERE DARBUOTOJAS.PROJEKTAS_ID = PROJEKTAS.ID 
AND PROJEKTAS.PAVADINIMAS = "Galerija"

3.
SELECT VARDAS, PAVARDĖ, PROJEKTAS.PAVADINIMAS AS PROJEKTAS, SKYRIUS.PAVADINIMAS AS SKYRIUS
FROM DARBUOTOJAS
JOIN PROJEKTAS ON DARBUOTOJAS.PROJEKTAS_ID = PROJEKTAS.ID AND PROJEKTAS.PAVADINIMAS = "Projektų valdymas"
JOIN SKYRIUS ON DARBUOTOJAS.SKYRIUS_ID = SKYRIUS.ID AND SKYRIUS.PAVADINIMAS = "Pardavimų" 

4.
SELECT VARDAS, PAVARDĖ, PROJEKTAS.PAVADINIMAS
FROM DARBUOTOJAS
JOIN PROJEKTAS ON DARBUOTOJAS.PROJEKTAS_ID = PROJEKTAS.ID AND PROJEKTAS.PAVADINIMAS = "Projektų valdymas"
WHERE DARBUOTOJAS.ASMENS_KODAS like("4%") OR DARBUOTOJAS.ASMENS_KODAS like("6%")

5.
SELECT SKYRIUS.PAVADINIMAS, count(*) as DARBUOTOJU_SK
FROM DARBUOTOJAS
JOIN SKYRIUS ON DARBUOTOJAS.SKYRIUS_ID = SKYRIUS.ID
GROUP BY SKYRIUS.PAVADINIMAS

6.
SELECT SKYRIUS.PAVADINIMAS, count(*) as DARBUOTOJU_SK
FROM DARBUOTOJAS
JOIN SKYRIUS ON DARBUOTOJAS.SKYRIUS_ID = SKYRIUS.ID
GROUP BY SKYRIUS.PAVADINIMAS
HAVING count() > 5

7.
SELECT VARDAS, PAVARDĖ, PAREIGOS, SKYRIUS.PAVADINIMAS AS SKYRIUS
FROM DARBUOTOJAS
JOIN SKYRIUS ON DARBUOTOJAS.SKYRIUS_ID = skyrius.ID
WHERE DARBUOTOJAS.PAREIGOS NOT like("Vadovas")

8.
INSERT INTO DARBUOTOJAS (VARDAS, PAVARDĖ, ASMENS_KODAS, DIRBA_NUO) 
VALUES("Pranas", "Logis", 38807117896, date('2009-11-12'))

9.
SELECT VARDAS, PAVARDĖ, SKYRIUS.PAVADINIMAS
FROM DARBUOTOJAS
LEFT JOIN SKYRIUS ON DARBUOTOJAS.SKYRIUS_ID = SKYRIUS.ID

10.
SELECT DARBUOTOJAS.VARDAS, DARBUOTOJAS.PAVARDĖ, PROJEKTAS.PAVADINIMAS
FROM DARBUOTOJAS
JOIN PROJEKTAS ON DARBUOTOJAS.PROJEKTAS_ID = PROJEKTAS.ID
WHERE DARBUOTOJAS.PROJEKTAS_ID IN
(SELECT PROJEKTAS_ID 
FROM DARBUOTOJAS
GROUP BY PROJEKTAS_ID
HAVING count() > 4)

11.
SELECT *, BAZINIS_ATLYGINIMAS+PRIEDAI AS GALUTINE_SUMA
FROM DARBUOTOJAS

12.
SELECT sum(BAZINIS_ATLYGINIMAS) AS ATLYGINIMO_SUMA, 
sum(PRIEDAI) AS PRIEDU_SUMA, 
avg(BAZINIS_ATLYGINIMAS) AS VIDUTINIS_ATLYGINIMAS, 
max(BAZINIS_ATLYGINIMAS) AS DIDZIAUSIAS_ATLYGINIMAS, 
min(BAZINIS_ATLYGINIMAS) AS MAZIAUSIAS_ATLYGINIMAS
FROM DARBUOTOJAS
